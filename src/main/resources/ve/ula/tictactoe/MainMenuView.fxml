<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="10" fx:id="container" xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="ve.ula.tictactoe.controllers.MainMenuViewController"
            prefHeight="400.0" prefWidth="600.0">
        <VBox alignment="CENTER" spacing="100">
            <Label text="TicTacToe" />
            <HBox alignment="CENTER" spacing="100">
                <Button fx:id="buttonLocal" text="Play Local"  />
                <Button fx:id="buttonOnline" text="Play Online"  />
            </HBox>
        </VBox>
</VBox>
